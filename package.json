{
  "name": "n8n-nodes-google-files-api",
  "version": "1.0.0",
  "description": "n8n nodes for Google Gemini File API and File Search Store",
  "keywords": [
    "n8n-community-node-package",
    "google",
    "gemini",
    "files-api",
    "rag",
    "file-search"
  ],
  "license": "MIT",
  "homepage": "",
  "author": {
    "name": "Author",
    "email": ""
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "engines": {
    "node": ">=18.17.0"
  },
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier --write .",
    "lint": "eslint . --ext .ts",
    "lintfix": "eslint . --ext .ts --fix",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/GoogleGeminiApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/GoogleFileApi/GoogleFileApi.node.js",
      "dist/nodes/GoogleFileSearchStore/GoogleFileSearchStore.node.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "eslint": "^8.56.0",
    "gulp": "^4.0.2",
    "n8n-workflow": "^1.22.0",
    "prettier": "^3.1.0",
    "typescript": "^5.3.0"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
